{
  "compilerOptions": {
    "outDir": "dist",
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true
  },
  "include": ["src"],"exclude": ["dist", "build", "node_modules"]
}